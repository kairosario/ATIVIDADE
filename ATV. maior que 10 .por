programa {
	funcao inicio() {
		inteiro n1 
		inteiro n2 
		inteiro resultado
		
		escreva ("insira n1")
		leia (n1)
		
		escreva ("insira n2")
		leia (n2)
		
		resultado = n1+n2 
		se (resultado > 10 )
		{
		    escreva (resultado+ " maior que 10 ")
		}
		senao 
		{
		    escreva ("menor que 10")
		}
	}
}
