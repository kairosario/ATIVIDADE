programa {
	funcao inicio() {
		inteiro n1
		inteiro n2
		inteiro resultado
		
		escreva ("insira o n1")
		leia (n1)
		
		escreva ("insira o n2")
		leia (n2)
		
		resultado = n1 % n2
		se (resultado != 0 )
		{
		    escreva (n1+ " não divisivel por "+n2)
		}
		senao 
		{
		    escreva (n1+ " divisivel por "+n2)
		}
	}
}
