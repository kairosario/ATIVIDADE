programa {
	funcao inicio() {
		inteiro n1
		inteiro n2
		inteiro resultado 
		
		escreva ("insira n1")
		leia (n1)
		
		escreva ("insira n2")
		leia (n2)
		
		resultado = n1 + n2
		se (resultado > 20 )
		{
		    resultado = resultado + 8
		    escreva (resultado)
		}
		se (resultado <= 20 )
		{
		    resultado = resultado - 5
		    escreva (resultado)
		}
	}
}
